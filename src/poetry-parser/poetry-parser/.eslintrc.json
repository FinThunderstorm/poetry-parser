{
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["prettier", "jest"],
    "rules": {
        "prettier/prettier": [
            "error",
            { "singleQuote": true, "semi": false, "tabWidth": 4 }
        ],
        "no-unused-vars": "error",
        "no-console": "error",
        "class-methods-use-this": "error"
    },
    "env": {
        "jest/globals": true
    }
}
